#!/usr/bin/env bash
# @BP010: Release metadata
# @package: mush-embed
# @build_type: bin
# @build_with: Mush 0.2.0 (2025-04-20 develop)
# @build_date: 2025-04-28T10:40:47Z
set -e
use() { return 0; }
extern() { return 0; }
legacy() { return 0; }
module() { return 0; }
public() { return 0; }
embed() { return 0; }
inject() { return 0; }
## BP004: Compile the entrypoint

#extern package console

embed pluto

#embed console
#embed alias_for_console console

main() {
  pluto sample.txt > main.txt
  #alias_for_pluto
  #alias_for_main > main.txt
  #manifest
  #console
  #alias_for_console
}
pluto() {
  if [ -z "$1" ]; then
    cat <<'EMBED'
inject file sample.txt
inject env sample_var
pluto() {
  test
}
EMBED
  else
    case "$1" in
      sample.txt)
        cat <<'EMBED'

Test!


EMBED
        ;;
      sample_var)
        echo ""
        ;;
      *)
        echo ": "
        ;;
    esac
  fi
}
## BP005: Execute the entrypoint
main "$@"
